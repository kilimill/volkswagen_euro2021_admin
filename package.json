{
	"name": "rpoint-boilerplate",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"core-js": "^2.6.5",
		"json2xls": "^0.1.2",
		"moment": "^2.29.1",
		"vue": "^2.6.10",
		"vue-cool-lightbox": "^2.1.1",
		"vue-json-excel": "^0.3.0",
		"vue-moment": "^4.1.0",
		"vue-router": "^3.0.3",
		"vue-sliding-pagination": "^1.0.4",
		"vuex": "^3.1.2",
		"vuex-persistedstate": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.8.0",
		"@vue/cli-plugin-eslint": "^3.8.0",
		"@vue/cli-service": "^3.8.0",
		"babel-eslint": "^10.0.1",
		"body-scroll-lock": "^2.6.4",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"esm": "^3.2.25",
		"pug": "^2.0.4",
		"pug-plain-loader": "^1.0.0",
		"style-resources-loader": "^1.2.1",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"vue-svg-loader": "^0.12.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-control-regex": 1
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 2%",
		"not dead",
		"not OperaMini all",
		"ie >= 10",
		"Edge >= 12"
	]
}
